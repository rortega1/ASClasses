/*	*/package {		import flash.net.URLRequest;	import flash.net.URLLoader;	import flash.events.*;		public class XMLParse extends EventDispatcher  	{		private var address:String;		private var xml:XMLList;		private var urlRequest:URLRequest;		private var urLoader:URLLoader;		public function XMLParse(url:String):void		{			address = url;			urlRequest = new URLRequest(address);			urLoader = new URLLoader();			urLoader.load(urlRequest);			urLoader.addEventListener(Event.COMPLETE, urLoader_complete);		}		private function urLoader_complete(evt:Event):void		{			xml = new XMLList(evt.currentTarget.data);			trace("4:  " + xml + "\n");			dispatchEvent(new Event(Event.COMPLETE));					}		public function displayTextArea(event:Event):XMLList		{			return xml;		}	}}